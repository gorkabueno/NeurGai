<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
   
    android:orientation="vertical" >

    <Button
        android:id="@+id/boton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Button"
        android:visibility="gone" />

    <TextView
        android:id="@+id/texto1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Potencia (W)"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/TextVPVPC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PVPC"
			android:visibility="gone" />

        <TextView
            android:id="@+id/TextV20APVPC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:text=" 2.0A " 
            android:visibility="gone"/>
        
        <TextView
        android:id="@+id/text20APVPC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:visibility="gone"
             />

        <TextView
            android:id="@+id/textV20DHAPVPC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:text=" 2.0DHA "
            android:visibility="gone" />

        
        <TextView
        android:id="@+id/text20DHAPVPC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:visibility="gone" 
            />
        
        <TextView
            android:id="@+id/textV20DHSPVPC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:text=" 2.0DHS "
            android:visibility="gone" />

         <TextView
             android:id="@+id/text20DHSPVPC"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_weight="0.142857142857"
             android:visibility="gone" 
             />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/TextVLibre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
             
            android:text="Libre"
            android:visibility="gone" />

        <TextView
            android:id="@+id/TextV20A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:text=" 2.0A "
            android:visibility="gone" />
        
        <TextView
        android:id="@+id/text20A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:visibility="gone"
             />

        <TextView
            android:id="@+id/textV20DHA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:text=" 2.0DHA "
            android:visibility="gone" />

        
        <TextView
        android:id="@+id/text20DHA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:visibility="gone" 
            />
        
        
        <TextView
            android:id="@+id/textV20DHS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.142857142857"
            android:text=" 2.0DHS "
            android:visibility="gone"/>

         <TextView
             android:id="@+id/text20DHS"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_weight="0.142857142857"
             android:visibility="gone" 
             />

    </LinearLayout>	
    <TextView
        android:id="@+id/texto4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="texto 4"
        android:visibility="gone" />

    <TextView
        android:id="@+id/texto5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="notas"
        android:visibility="gone" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone" />
